{
  "name": "@gera2ld/jar-parser",
  "scripts": {
    "dev": "rollup -wc rollup.conf.js",
    "clean": "del dist",
    "build:js": "rollup -c rollup.conf.js",
    "prebuild": "npm run ci && npm run clean",
    "prepublishOnly": "npm run build",
    "ci": "npm run lint && npm run test",
    "build": "npm run build:js",
    "lint": "eslint .",
    "test": "cross-env BABEL_ENV=test tape -r ./test/mock/register 'test/**/*.test.js'",
    "cov": "nyc --reporter=text --reporter=html npm test",
    "cov:open": "open coverage/index.html"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "jszip": "^3.2.2"
  },
  "resolutions": {
    "eslint": "~6.1.0"
  },
  "version": "2.2.1",
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "nyc": {
    "include": [
      "src/**"
    ]
  },
  "devDependencies": {
    "@babel/register": "^7.5.5",
    "@gera2ld/plaid": "~1.4.0",
    "babel-plugin-istanbul": "^5.2.0",
    "cross-env": "^5.2.0",
    "del-cli": "^2.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "husky": "^3.0.4",
    "jszip": "^3.2.2",
    "nyc": "^14.1.1",
    "rollup": "^1.20.2",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "tape": "^4.11.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5"
  },
  "repository": "git@github.com:gera2ld/jar-parser.git",
  "author": "Gerald <gera2ld@163.com>",
  "license": "MIT"
}
